{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "LOCAL_DEV=true node dev-server.js",
    "frontend": "vite",
    "build": "npx vite build --mode production",
    "build:staging": "npx vite build --mode staging",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:functions:config": "firebase functions:config:set plaid.env=production plaid.client_id=\"$PLAID_CLIENT_ID\" plaid.secret=\"$PLAID_SECRET\"",
    "deploy:frontend": "npm run build && netlify deploy --prod",
    "deploy:staging": "npm run build:staging && netlify deploy --site $NETLIFY_STAGING_SITE_ID --auth $NETLIFY_AUTH_TOKEN",
    "deploy": "npm run build && npm run deploy:functions && netlify deploy --prod",
    "deploy:production": "npm run deploy:functions:config && npm run deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
